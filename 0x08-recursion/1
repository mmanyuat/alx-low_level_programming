#include "main.h"
#include <stdio.h>
#include <math.h>
/**
 * _pow_recursion - function to print power of a base to exp
 * @x:the base number
 * @y:the exponent
 * Return: 0 , result
 */

int _pow_recursion(int x, int y)
{
	int res = _pow_recursion(x, y);

	if (y < 0)
	{
	return (0);
	}
	return (res);
}
